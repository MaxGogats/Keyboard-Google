{
	"manifest_version": 3,
	"name": "no mouse google", 
	"description": "Base Level Extension",
	"version": "1.0",
	"action": {
        "default_popup": "no-mouse.html",
	    "default_icon": "images/hello_extensions.png"
	},
	"content_scripts": [
	  {
		"js": ["content.js"],
		"matches": ["*://www.google.com/*search*"] 
	  }
	]
}